<h2>ジャンル一覧・追加</h2>
<%=form_with model: @genre,url: admin_genrse_index_path do |f| %>
  <h4>ジャンル名</h4>
  <%=f.text_field :name %>
  
  <%=f.submit '新規登録' %>
<%end%>

<table>
  <thead>
    <tr>
      <td><strong>ジャンル</strong></td>
      <td></td>
    </tr>
  </thead>
  <tbody>
    <% @genrse.each do |genre| %>
      <tr>
        <td><%=genre.name %></td>
        <td><%=link_to "編集する",edit_admin_genrse_path(genre.id) %></td>
      </tr>
    <%end%>
  </tbody>
</table>
